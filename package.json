{
  "name": "react-shop-components",
  "version": "1.0.2",
  "description": "sandbox to play around with storybook and testing isolated components.",
  "main": "dist/react-shop-components.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
		"plugins": ["@babel/plugin-proposal-class-properties"]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "no-tabs": 0,
      "indent": [
        1,
        "tab"
      ],
      "react/jsx-indent": [
        "error",
        "tab"
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
  },
  "keywords": [
    "test",
    "dev",
    "storybook",
    "react",
    "components"
  ],
  "author": "Fredi Quirino",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "classnames": "^2.2.6",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-bulma-components": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.1.9",
    "@storybook/addon-links": "^4.1.9",
		"@storybook/addon-knobs": "^4.1.11",
    "@storybook/addon-storysource": "^4.1.11",
    "@storybook/addons": "^4.1.11",
    "@storybook/react": "^4.1.9",
    "autoprefixer": "^9.4.8",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^1.2.2",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  }
}
